#include<iostream>
using namespace std ;

bool isPalindrome(int arr[], int left, int right)  // FIX: function name typo corrected (was isPalindeome)
{
    if(left >= right)
        return true;
    if(arr[left] != arr[right])
        return false;
    return isPalindrome(arr, left+1, right-1) ;
}
int main()
{
    int len ; cin>>len ;
    int arr[len] ;
    for(int i=0; i<len; i++)
    {
        cin>>arr[i] ;
    }

    if(isPalindrome(arr, 0, len-1) )
        cout<<"palindrome\n";
    else
        cout<<"not palindrome\n";
    return 0 ;
}
